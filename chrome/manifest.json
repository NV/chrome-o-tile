{
	"name": "Tile-o-some",
	"version": "1.2.2",
	"description": "Opens new windows right next to the current. Donâ€™t overlap.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"js": ["tile-o-some.js"],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"icons": {
		"32": "icon_32.png",
		"48": "icon_48.png"
	},
	"permissions": ["tabs"],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"update_url": "http://clients2.google.com/service/update2/crx"
}
